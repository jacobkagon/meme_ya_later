<h1>Sign Up</h1>
<%= form_for @user do |f|%>
<%= f.label :name%><br>
<%=f.text_field :name%><br>
<%= f.label :username%><br>
<%= f.text_field :username%><br>
<%= f.label :email%><br>
<%= f.text_field :email_address%><br>
<%= f.label :password%><br>
<%= f.password_field :password %><br>
<%= f.label :password_confirmation%><br>
<%= f.password_field :password_confirmation%><br>
<%= f.submit %>
<%end%>

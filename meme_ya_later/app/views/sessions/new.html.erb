


<h1>Login</h1>
   <div>
   <%= form_tag login_path, method: 'post' do %>
   <div class = 'danger'>
   <%=flash[:danger]%>
   </div>
   
<div class="actions">
   <%= label_tag :username%>
   <%= text_field_tag :username %>
   <%= label_tag :password%>
   <%= password_field_tag :password%>
   <%= submit_tag "Login"%>
<%end%><br>
</div>
